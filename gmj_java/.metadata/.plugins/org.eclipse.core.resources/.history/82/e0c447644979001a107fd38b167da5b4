		int list[] = {4,3,2,1,8,7,5,10,11,16,21,6};
		int sub=0,sub_list=0;
		for(int i=0;i<list.length;i++){
			for(int j=0; j<(list.length-1);j++){
				sub_list=list[j+1];
				if(list[j]>sub_list){
					sub = sub_list;
					sub_list=list[j];
					list[j]=sub;
				}
				list[j+1]=sub_list;
			}
		}
		for(int i=0;i<list.length;i++){
			System.out.print(list[i]+" ");
		}
