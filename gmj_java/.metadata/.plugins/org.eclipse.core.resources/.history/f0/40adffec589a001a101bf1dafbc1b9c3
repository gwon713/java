package a111112;

import java.util.Scanner;

public class T1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        for(int i=0; i<b; i++){
        	for(int j=0; j<a; i++){
        		System.out.print("*");
        	}
        	System.out.println();
        }

        System.out.println(a + b);
	}

}
