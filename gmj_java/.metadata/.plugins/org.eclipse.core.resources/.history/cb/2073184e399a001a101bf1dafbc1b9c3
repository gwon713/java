package a111112;

import java.util.Scanner;

public class T1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();
        String star = "*";
        star+=a;
        for(int i=0; i<b; i++){
        	System.out.println(star);
        }
        System.out.print("*");
        System.out.println(a + b);
	}

}
